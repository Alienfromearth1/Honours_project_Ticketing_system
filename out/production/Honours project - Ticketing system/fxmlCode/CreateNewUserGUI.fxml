<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javaControllerCode.CreateUserGUIController">
   <children>
      <Pane prefHeight="640.0" prefWidth="887.0">
         <children>
             <Label layoutX="100.0" layoutY="100.0" text="First Name:" />
             <Label layoutX="100.0" layoutY="150.0" text="Surname:" />
             <Label layoutX="100.0" layoutY="200.0" text="Username:" />
             <Label layoutX="100.0" layoutY="250.0" text="Password:" />
             <Label layoutX="100.0" layoutY="300.0" text="Confirm Password:" />
             <Button fx:id="btnMenu" layoutX="130.0" layoutY="500.0" mnemonicParsing="false" onAction="#ReturnToMenu" prefHeight="75.0" prefWidth="150.0" text="Menu" />
             <Button fx:id="btnCreateUser" layoutX="607.0" layoutY="500.0" mnemonicParsing="false" onAction="#CreateUser" prefHeight="75.0" prefWidth="150.0" text="Create User" />
             <TextField fx:id="txtFname" layoutX="350.0" layoutY="100.0" />
             <TextField fx:id="txtSname" layoutX="350.0" layoutY="150.0" />
             <TextField fx:id="txtUserName" layoutX="350.0" layoutY="200.0" />
            <CheckBox fx:id="chkTechnician" layoutX="350.0" layoutY="350.0" mnemonicParsing="false" text="User is technician" />
            <PasswordField fx:id="txtPassword" layoutX="350.0" layoutY="250.0" />
            <PasswordField fx:id="txtConfirmPass" layoutX="350.0" layoutY="300.0" />
         </children></Pane>
   </children>
</AnchorPane>
